<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="StudFood.Views.ListPage">
  
  <StackLayout VerticalOptions="Center">

    <Label Text="Продукты" HorizontalOptions="Center" VerticalOptions="Center" FontSize="20"></Label>
    <Label Text="Хлеб" HorizontalOptions="Center" VerticalOptions="Center" FontSize="15"></Label>
    <Label Text="Сыр" HorizontalOptions="Center" VerticalOptions="Center" FontSize="15"></Label>
    <Label Text="Гречка" HorizontalOptions="Center" VerticalOptions="Center" FontSize="15"></Label>
    <Label Text="Картофель" HorizontalOptions="Center" VerticalOptions="Center" FontSize="15"></Label>
    <Label Text="Молоко" HorizontalOptions="Center" VerticalOptions="Center" FontSize="15"></Label>
    <Label Text="Рис" HorizontalOptions="Center" VerticalOptions="Center" FontSize="15"></Label>
    <Label Text="Яйца" HorizontalOptions="Center" VerticalOptions="Center" FontSize="15"></Label>
    <Label Text="Мука" HorizontalOptions="Center" VerticalOptions="Center" FontSize="15"></Label>
    <Label Text="Хлопья" HorizontalOptions="Center" VerticalOptions="Center" FontSize="15"></Label>
    <Label Text="Сахар" HorizontalOptions="Center" VerticalOptions="Center" FontSize="15"></Label>
    <Button Text="Далее" Command="{Binding NextCommand}" />

 
  </StackLayout>

</ContentPage>